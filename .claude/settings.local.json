{
  "permissions": {
    "allow": [
      "mcp__serena__think_about_whether_you_are_done"
    ],
    "deny": [],
    "ask": []
  },
  "outputStyle": "default",
  "spinnerTipsEnabled": true,
  "autoCompactEnabled": false,
  "mcpServers": {
    "context7": {
      "command": "C:/Program Files/nodejs/npx.cmd",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "SYSTEMROOT": "C:/Windows"
      }
    },
    "playwright": {
      "command": "C:/Program Files/nodejs/npx.cmd",
      "args": [
        "-y",
        "@playwright/mcp@latest",
        "--browser",
        "chromium"
      ],
      "env": {
        "SYSTEMROOT": "C:/Windows"
      }
    },
    "serena": {
      "command": "C:/Users/保科　慶光/.local/bin/uvx.exe",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "claude"
      ],
      "env": {
        "SYSTEMROOT": "C:/Windows"
      }
    },
    "github": {
      "command": "C:/Program Files/nodejs/npx.cmd",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "SYSTEMROOT": "C:/Windows"
      }
    },
    "plantuml": {
      "command": "C:/Program Files/nodejs/npx.cmd",
      "args": [
        "-y",
        "plantuml-mcp-server"
      ],
      "env": {
        "SYSTEMROOT": "C:/Windows"
      }
    },
    "plantuml-validator": {
      "command": "C:/Program Files/nodejs/node.exe",
      "args": [
        "C:/d/plantuml-validator-mcp-server/dist/index.js"
      ],
      "env": {
        "SYSTEMROOT": "C:/Windows"
      }
    },
    "claude-ops-mcp": {
      "command": "C:/Program Files/nodejs/npx.cmd",
      "args": [
        "-y",
        "claude-ops-mcp"
      ],
      "env": {
        "SYSTEMROOT": "C:/Windows"
      }
    }
  }
}
